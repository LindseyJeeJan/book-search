{
  "name": "googlebooks-app",
  "version": "1.0.0",
  "description": "application for searching google books API and saving to your account",
  "main": "server/server.js",
  "engines": {
    "npm": "7.x"
  },
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LindseyJeeJan/book-search.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "bugs": {
    "url": "https://github.com/LindseyJeeJan/book-search/issues"
  },
  "homepage": "https://github.com/LindseyJeeJan/book-search#readme"
}
